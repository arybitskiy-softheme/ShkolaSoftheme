<Window x:Class="age_Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:age_Wpf"
        mc:Ignorable="d"
        Title="Zodiac" Height="214.308" Width="458.231">
    <Grid Margin="10,0,3.4,-0.2">
        <Image x:Name="image" HorizontalAlignment="Left" Height="100" Margin="320,72,0,0" VerticalAlignment="Top" Width="100"/>
        <Button x:Name="button" Content="Magic" HorizontalAlignment="Left" Margin="320,22,0,0" VerticalAlignment="Top" Width="100" RenderTransformOrigin="-1.453,-0.131" Height="23" Click="button_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="10,22,0,0" TextWrapping="Wrap" Text="Enter your date of birth" VerticalAlignment="Top" Width="263" TextChanged="textBox_TextChanged"/>
        <Label x:Name="label" Content="Your age:" HorizontalAlignment="Left" Margin="10,83,0,0" VerticalAlignment="Top" Width="75"/>
        <Label x:Name="label_Copy" Content="Your zodiac:" HorizontalAlignment="Left" Margin="10,114,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
